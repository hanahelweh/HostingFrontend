<script>
    st(0);
    $('section').each(function () {
    $(".liveTracker").append(`<div class="navi p5 text2 pointer colorHover ez-f fs16 ` + (this.id) + `" @click="st($('#` + (this.id) + `').offset().top - 95);">` + (this.title) + `</div>`);
});
$(window).scroll(function () {
$("section").each(function () {
    let next = $(this).next("section");
    let startX = $(this).offset().top - window.pageYOffset;
    let cond = next.length
    ? startX <= 95 && next.offset().top - window.pageYOffset > 95
    : startX <= 95;
    if (cond) {
    if ($(".liveTracker").length)
        $(".liveTracker .navi." + this.id)
        .removeClass("text2")
        .addClass("active text1");
    } else {
    if ($(".liveTracker").length)
        $(".liveTracker .navi." + this.id)
        .removeClass("active")
        .addClass("text2");
    }
});
});
</script>